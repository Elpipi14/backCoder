<div id="cart">
    {{#each products}}
        <div class="cart-item">
            <img src="{{this.product.imageUrl}}" alt="{{this.product.title}}" class="product-image">
            <h3 class="product_title">{{this.product.title}}</h3>
            <div class="product-details">
                <div class="cart_quantity">
                    <h2>Cantidad</h2>
                    <div class="aum_rest">
                        <button class="btn cart px-auto">-</button>
                        <span>{{this.quantity}}</span>
                        <button class="btn cart px-auto">+</button>
                    </div>
                </div>
                <div class="cart_price">Precio: <span class="price">{{this.product.price}} $</span></div>
                <button class="delete-btn"><box-icon name="trash-alt" type="solid" animation="tada" color="#7749c7"></box-icon></button>
            </div>
        </div>
    {{/each}}
    <div class="cart_btn">
        <button class="btn cart px-auto fin">Add Products</button>
        <button class="btn cart px-auto fin">CheckOut</button>
    </div>
</div>